{
  "name": "aconitum",
  "version": "0.0.0",
  "description": "",
  "main": "./build/index.js",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "build": "rimraf build/ && tsc",
    "pm2-dev": "pm2 start ecosystem.config.cjs --env dev",
    "pm2-prod": "pm2 start ecosystem.config.cjs --env prod",
    "start-dev": "cross-env NODE_ENV=development node .",
    "start-prod": "cross-env NODE_ENV=production node .",
    "cmd-dev": "cross-env NODE_ENV=development node build/deployCommands.js",
    "cmd-prod": "cross-env NODE_ENV=production node build/deployCommands.js",
    "tsx": "npm run type && cross-env NODE_ENV=development node --import=tsx ./src/index.ts",
    "tsx-watch": "npm run type && cross-env NODE_ENV=development tsx watch ./src/index.ts",
    "tsx-cmd": "npm run type && cross-env NODE_ENV=development node --import=tsx ./src/deployCommands.ts",
    "type": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EnterTeamNameHere/Aconitum.git"
  },
  "author": "EnterTeamNameHere",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EnterTeamNameHere/Aconitum/issues"
  },
  "homepage": "https://github.com/EnterTeamNameHere/Aconitum#readme",
  "dependencies": {
    "@line/bot-sdk": "^9.0.1",
    "@types/express": "^4.17.21",
    "@vladfrangu/async_event_emitter": "^2.2.4",
    "axios": "^1.6.7",
    "cross-env": "^7.0.3",
    "discord-api-types": "^0.37.71",
    "discord.js": "^14.12.1",
    "express": "^4.18.3",
    "mongodb": "^6.4.0",
    "ramda": "^0.29.1",
    "node-cron": "^3.0.3",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/ramda": "^0.29.11",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-base-typescript": "^1.1.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "volta": {
    "node": "20.11.1",
    "npm": "10.5.0"
  },
  "lint-staged": {
    "src/**/*.{ts,mts,mts}": "eslint --fix",
    "src/**/*.{ts,cts,mts,json,yaml,yml,md}": "prettier --write"
  }
}
